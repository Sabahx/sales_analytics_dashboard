[run]
source = src
omit =
    */tests/*
    */test_*.py
    */__init__.py
    */config/__pycache__/*
    */__pycache__/*
    */venv/*
    */env/*

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
